<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox spacing="4.0" styleClass="apdu-response" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.vakho10.apdutracer.apdu.response.Controller">
   <children>
      <VBox spacing="2.0" HBox.hgrow="ALWAYS">
         <children>
            <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Response data" />
            <TextField fx:id="fieldResponseData" alignment="CENTER" editable="false" prefWidth="24.0" />
         </children>
      </VBox>
      <VBox spacing="2.0">
         <children>
            <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Status" />
            <TextField fx:id="fieldStatus" alignment="CENTER" editable="false" prefWidth="48.0" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</HBox>
